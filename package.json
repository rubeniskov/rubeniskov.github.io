{
  "name": "rubeniskov-website",
  "version": "0.1.0",
  "private": true,
  "description": "http://rubeniskov.com webpage",
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/resume-parser/transforms/*",
      "plugins/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubeniskov/rubeniskov.github.io.git"
  },
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@rubeniskov/markdown": "^0.0.1",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-link-context": "^1.0.20",
    "apollo-link-rest": "^0.8.0-beta.0",
    "clsx": "^1.1.1",
    "graphql": "^15.4.0",
    "graphql-anywhere": "^4.2.7",
    "graphql-tag": "^2.11.0",
    "monaco-editor": "^0.21.2",
    "monaco-editor-webpack-plugin": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-suspense": "^0.0.5",
    "sitemap": "^6.3.3",
    "styled-components": "^5.2.1",
    "styled-theming": "^2.2.0",
    "suspense-redux": "^0.1.1",
    "use-http": "^1.0.15",
    "uuid": "^8.3.1",
    "web-vitals": "^0.2.4"
  },
  "homepage": "http://rubeniskov.com",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "eslint": "eslint src",
    "bootstrap": "lerna bootstrap"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "^7.12.5",
    "@rubeniskov/webpack-render-static-routes-plugin": "^0.0.1",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "ify-loader": "rubeniskov/ify-loader",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6"
  }
}
